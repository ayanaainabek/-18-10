package labka;

public class Students {
 public static void main(String args[]) {
 TestThread myThread = new TestThread();
 myThread.start();
	}
}

class TestThread extends Thread{
 @Override
 public void run() {
 for (int i = 0; i <= 1000; i ++) {
 System.out.println(this.getName() + "\t" + i);
		}
	}
